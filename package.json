{
  "name": "sivananda-housekeeping",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/danthareja/sivananda-housekeeping.git",
  "author": "Dan Thareja <danthareja@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "NODE_ENV=development nodemon index.js --ignore client --ignore bin",
    "dev:client": "cd client && npm start",
    "postinstall": "cd client && npm install && npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "apollo-datasource-rest": "^3.4.0",
    "apollo-server-caching": "^3.3.0",
    "apollo-server-express": "^3.5.0",
    "axios": "^0.24.0",
    "cachegoose": "^8.0.0",
    "csv-parse": "^5.0.3",
    "express": "^4.17.1",
    "graphql": "^15.8.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.5",
    "lodash": "^4.17.21",
    "merge-graphql-schemas": "^1.5.7",
    "moment": "^2.29.1",
    "mongoose": "^6.1.2"
  },
  "devDependencies": {
    "concurrently": "^6.4.0",
    "dotenv": "^10.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "nodemon": "^2.0.15",
    "prettier": "2.5.1"
  }
}
